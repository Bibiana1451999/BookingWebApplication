@model IEnumerable<BookingWebApp.Models.r_room>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.r_beds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.r_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.r_description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.h_hotel.h_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tr_typeOfRoom.tr_type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.r_beds)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.r_price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.r_description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.h_hotel.h_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tr_typeOfRoom.tr_type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.r_number }) |
            @Html.ActionLink("Details", "Details", new { id=item.r_number }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.r_number })
        </td>
    </tr>
}

</table>
